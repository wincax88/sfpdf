<apex:page>
<html style="height:100%;">
<head>
    <meta http-equiv="Content-Type" content="text/html"></meta>
    
    <script src="{!URLFOR($Resource.pdftronjs, 'lib/html5/external/jquery-1.7.2.min.js')}"></script>
    <script src="{!URLFOR($Resource.pdftronjs, 'lib/WebViewer.min.js')}"></script>
    <script type="text/javascript">

    $(function() {
        var queryParams = window.ControlUtils.getQueryStringMap(false);
        var docType = queryParams.getString('doctype', 'xod');

        var viewerElement = document.getElementById('viewer');
        var myWebViewer = new PDFTron.WebViewer({
            type: "html5",
            path: "../../lib",
            initialDoc: "https://pdftron.s3.amazonaws.com/files/xod/NY_fun_list." + docType,
            documentType: docType,
            useDownloader: false,
            enableAnnotations: true,
            html5Path: "html5/ReaderControl.html",
            html5MobilePath: "html5/MobileReaderControl.html"
        }, viewerElement);
    });
    </script>
</head>

<body style="width:100%;height:100%;margin:0px;padding:0px;overflow:hidden">
    <div id="viewer" style="height: 100%; overflow: hidden;"></div>
</body>
</html>
</apex:page>